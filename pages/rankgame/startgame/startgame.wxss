Page{
  width: 100%;
  height: 100%;

}
.keyWord{
  width: 600rpx;
  margin: 40rpx auto;
  background:#fff;
  border-top: 1rpx solid #ddd;
}

.keyWord text{
  height: 100rpx;
  line-height: 100rpx;
  border-right: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}

.line {
  border-left: 1rpx solid #ddd;
}

.flex-row{
  display: flex;
  flex: 1;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: stretch;
  align-items: center;
}